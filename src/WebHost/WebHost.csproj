<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AssemblyName>NetModular.Demo.WebHost</AssemblyName>
    <RootNamespace>NetModular.Demo.WebHost</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NetModular.Lib.Cache.MemoryCache" Version="1.9.11" />
    <PackageReference Include="NetModular.Lib.Cache.Redis" Version="1.9.11" />
    <PackageReference Include="NetModular.Lib.Data.MySql" Version="1.9.12" />
    <PackageReference Include="NetModular.Lib.Excel.EPPlus" Version="1.9.4" />
    <PackageReference Include="NetModular.Lib.Host.Web" Version="1.9.9" />
    <PackageReference Include="NetModular.Lib.Utils.Core" Version="1.9.5" />
    <PackageReference Include="NetModular.Module.Admin.Web" Version="1.9.7" />
    <PackageReference Include="NetModular.Module.CodeGenerator.Web" Version="1.7.4" />
    <PackageReference Include="NetModular.Module.Common.Web" Version="1.6.8" />
    <PackageReference Include="NetModular.Module.PersonnelFiles.Web" Version="1.6.0" />
    <PackageReference Include="NetModular.Module.Quartz.Web" Version="1.6.3" />
  </ItemGroup>

  <!--复制配置文件到输出目录-->
  <ItemGroup>
    <Content Update="config\**">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
