<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <AssemblyName>NetModular.Demo.WebHost</AssemblyName>
    <RootNamespace>NetModular.Demo.WebHost</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Nm.Lib.Cache.MemoryCache" Version="1.4.0" />
    <PackageReference Include="Nm.Lib.Data.MySql" Version="1.4.0" />
    <PackageReference Include="Nm.Lib.Data.SQLite" Version="1.4.0" />
    <PackageReference Include="Nm.Lib.Data.SqlServer" Version="1.4.0" />
    <PackageReference Include="Nm.Lib.Host.Web" Version="1.4.0" />
    <PackageReference Include="Nm.Module.Admin.Web" Version="1.4.0" />
    <PackageReference Include="Nm.Module.CodeGenerator.Web" Version="1.4.0" />
    <PackageReference Include="Nm.Module.Common.Web" Version="1.4.0" />
    <PackageReference Include="Nm.Module.PersonnelFiles.Web" Version="1.4.0" />
    <PackageReference Include="Nm.Module.Quartz.Web" Version="1.4.0" />
  </ItemGroup>

  <!--复制配置文件到输出目录-->
  <ItemGroup>
    <Content Update="config\**">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
